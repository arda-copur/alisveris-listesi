@using Entities;
@model List<UserProductList>

<!DOCTYPE html>
<html>
<head>
    <title>UserProduct List</title>
</head>
<body>
   
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Name</th>

        </tr>
        @foreach (var userproductlist in Model)
        {
            <tr>
                <td>@userproductlist.Id</td>
                <td>@userproductlist.Product.ÜrünAdı</td>


            </tr>
        }
    </table>
    <a href="/UserProductList/AddUserProductList/" class="btn btn-primary">ekle</a>
</body>
</html>
